SRC_DIR_fd1 = src/
OBJS_DIR = Release/obj/
EXE_DIR = Release/bin/

EXE = bulk
FC = ifort
IDIR =
CFLAGS = -O3 -mcmodel=medium -ipo -threads -save -mkl=parallel -assume buffered-io
LFLAGS = -qopenmp
LIBS = -lmatmul -lmkl_rt -lpthread -lm -ldl

VPATH = $(SRC_DIR_fd1):$(OBJS_DIR)
OBJS = $(addprefix $(OBJS_DIR), $(OBJS_fd1))

all : $(EXE)

$(EXE) : $(OBJS_fd1)
	@mkdir -p $(EXE_DIR)
	$(FC) -o $(EXE_DIR)$(EXE) $(OBJS) $(LFLAGS) $(LIBS)

$(OBJS_fd1):
	@mkdir -p $(OBJS_DIR)
	$(FC) $(CFLAGS) -c $(SRC_DIR_fd1)$(@:.o=.f) -o $(OBJS_DIR)$@

clean :
	rm -f $(OBJS_DIR)*.*
	rm -f $(EXE_DIR)$(EXE)

